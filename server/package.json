{
  "name": "pyski-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:dev": "DEBUG=pyski* nodemon ./server/index.ts --config ./nodemon.json",
    "start": "ts-node ./index.ts",
    "test": "mocha ./lib/**/*.test.ts --exit --timeout=10000 --require ts-node/register",
    "db:fixtures:load": "ts-node lib/database/fixtures/load.ts",
    "build": "npm run db:migration:run && npm run db:fixtures:load",
    "db:migration:create": "ts-node ./node_modules/.bin/typeorm migration:create",
    "db:migration:generate": "ts-node ./node_modules/.bin/typeorm migration:generate",
    "db:migration:run": "ts-node ./node_modules/.bin/typeorm migration:run",
    "db:migration:revert": "ts-node ./node_modules/.bin/typeorm migration:revert"
  },
  "dependencies": {
    "@decorators/express": "^2.4.0",
    "apollo-server": "^2.21.1",
    "apollo-server-express": "^2.21.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.3.5",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "method-override": "^2.3.6",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.10",
    "serve-favicon": "^2.5.0",
    "subscriptions-transport-ws": "^0.9.18",
    "ts-node": "^9.1.1",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.31",
    "typeorm-fixtures-cli": "^1.9.1",
    "underscore": "^1.12.0",
    "uniqid": "^5.3.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/morgan": "^1.9.2",
    "@types/underscore": "^1.10.24",
    "@types/uniqid": "^5.2.0",
    "husky": "^5.0.9",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7"
  }
}
